{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
  "schedule": "00 15 * * *"
    },
    {
      "path": "/api/cron-send",
      "schedule": "03 15 * * *"
    }
  ]
}
